{
  "name": "plejeune/user-bundle",
  "type": "symfony-bundle",
  "description": "Pierre LEJEUNE UserBundle",
  "keywords": [
    "Core Bundle"
  ],
  "repositories": [
    { "type": "gitlab", "url": "https://gitlab.com/pierrelejeune/corebundle.git" }
  ],
  "homepage": "https://gitlab.com/pierrelejeune/corebundle",
  "license": "proprietary",
  "authors": [
    {
      "name": "Pierre LEJEUNE",
      "email": "lejeune.pierre.41@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "knplabs/knp-menu-bundle": "^2.0",
    "plejeune/core-bundle":"dev-master",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/orm-pack": "*",
    "symfony/translation": "*",
    "symfony/twig-bundle": "*",
    "symfony/validator": "*",
    "symfony/yaml": "*"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0"
    }
  },
  "autoload": {
    "psr-4": {
      "PLejeune\\UserBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}